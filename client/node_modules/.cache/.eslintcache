  function handleTemperament(e){\r\n        e.preventDefault();\r\n        dispatch(orderTemperament(e.target.value));\r\n        console.log('se manda!')\r\n    }\r\n\r\n\r\n    useEffect(() => {\r\n        dispatch(getTemperament());\r\n    }, [dispatch])\r\n\r\n    return (\r\n        <ul className=\"ulMenu\">\r\n            <li><label className=\"lavelMenu\" >ordenamiento: </label><select \r\n                    className='selectMenu'\r\n                    onChange={handleSort}\r\n                > \r\n                <option className='optionMenu' value='asc'> Ascendente </option>\r\n                <option className='optionMenu' value='desc'> Descendente </option> \r\n            </select></li>\r\n\r\n            <li><label className=\"lavelMenu\">peso: </label><select\r\n                    className='selectMenu'\r\n                    onChange={handlePeso}\r\n                > \r\n                <option className='optionMenu' value='mayor'> Mayor Peso </option>\r\n                <option className='optionMenu' value='menor'> Menor Peso </option>\r\n            </select></li>\r\n\r\n            <li><label className=\"lavelMenu\">existentes: </label><select\r\n                    className='selectMenu'\r\n                    onChange={handleExistent}\r\n                > \r\n                <option className='optionMenu' value='all'>todos</option>\r\n                <option className='optionMenu' value='created'>creado</option>\r\n                <option className='optionMenu' value='existing'>existente</option> \r\n            </select></li>\r\n\r\n            <li>\r\n                <label className=\"lavelMenu\">Temperamento: </label>\r\n                <select\r\n                    className='selectMenu'\r\n                    onChange={handleTemperament}\r\n                    // onClick={e => handleTemperament(e)}\r\n                    name='temperament'\r\n                >\r\n                    <option value='all'>all</option>\r\n                    {\r\n                        AllTemperament?.map((el) =>{\r\n\r\n                            return (\r\n\r\n                                <option\r\n                                    className='optionMenu'\r\n                                    key={el.id}\r\n                                    name='temperament'\r\n                                    value={el.name}\r\n                                >\r\n                                    {el.name}\r\n                                </option>\r\n                            )\r\n                        })\r\n                    }    \r\n                </select>\r\n            </li>\r\n        </ul>\r\n\r\n        \r\n    )\r\n}\r\nexport default Nav;","C:\\Users\\Usuario\\Documents\\Henry\\Pi\\PI-Dogs-main\\client\\src\\Components\\Loader.jsx",[],"C:\\Users\\Usuario\\Documents\\Henry\\Pi\\PI-Dogs-main\\client\\src\\Components\\Background.jsx",[],{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","severity":1,"message":"80","line":22,"column":25,"nodeType":"81","messageId":"82","endLine":22,"endColumn":38},{"ruleId":"83","severity":1,"message":"84","line":25,"column":1,"nodeType":"85","messageId":"86","endLine":25,"endColumn":65},{"ruleId":"79","severity":1,"message":"87","line":20,"column":14,"nodeType":"81","messageId":"82","endLine":20,"endColumn":26},"no-native-reassign",["88"],"no-negated-in-lhs",["89"],"no-unused-vars","'setDogPerPage' is assigned a value but never used.","Identifier","unusedVar","no-lone-blocks","Block is redundant.","BlockStatement","redundantBlock","'handleChange' is defined but never used.","no-global-assign","no-unsafe-negation"]