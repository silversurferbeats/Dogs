ckground';\r\n\r\nimport Card from \"./Card\";\r\nimport Paginado from \"./Paginado\";\r\nimport Loader from \"./Loader\";\r\n\r\nfunction Home(){\r\n\r\n    const dispatch = useDispatch();\r\n    // getteo  el estado global de Redux:\r\n    const AllDogs = useSelector((state) => state.allDogs);\r\n\r\n    // PAGINADO:\r\n    const [ currentPage, setCurrentPage ] = useState(1);\r\n    const [ dogPerPage, setDogPerPage ] = useState(8);\r\n    const indexOfLastDog = currentPage * dogPerPage;\r\n    const indexOfFirstDog = indexOfLastDog - dogPerPage;\r\n    const currentDog = AllDogs?.slice(indexOfFirstDog, indexOfLastDog);\r\n\r\n\r\n    const handleAnterior = (e) => {\r\n        e.preventDefault();\r\n        if (currentPage > 1){\r\n            setCurrentPage(currentPage - 1);\r\n        }\r\n    }\r\n\r\n    const handleSiguiente = (e) => {\r\n        e.preventDefault();\r\n        let ValorPagina = Math.ceil(AllDogs.length / dogPerPage);\r\n        if(ValorPagina !== currentPage){\r\n            setCurrentPage(currentPage + 1);\r\n        }\r\n    }\r\n\r\n    const paginado = (pageNumber) => {\r\n        setCurrentPage(pageNumber);\r\n    }\r\n    //  ------------------>\r\n\r\n    useEffect(()=>{\r\n        dispatch(getAllDogs())\r\n    }, [dispatch]);\r\n\r\n    return (\r\n        <>\r\n        {/* <BackgroundBubble/> */}\r\n        <div>\r\n            <SearchBar />\r\n        </div>\r\n        <div className=\"bodyContainer\">\r\n            <div className=\"menuContainer\">\r\n                <Nav />\r\n            </div>\r\n            <div className=\"cardContainer\">\r\n                {\r\n                    currentDog.length ?\r\n                    currentDog?.map((el) => {\r\n                        return (\r\n                            <div key={el.id}>\r\n                                <Link to={'/detail/' + el.id}>\r\n                                    <Card  \r\n                                        id={el.id} \r\n                                        name={el.name} \r\n                                        temperament={el.temperament} \r\n                                        img={el.image} \r\n                                    />\r\n                                </Link>\r\n                            </div>\r\n                        )\r\n                    })\r\n                    :\r\n                    <div className=\"Loader\">\r\n                        <Loader/>\r\n                    </div>\r\n                }\r\n            </div>\r\n\r\n        </div>\r\n\r\n        <footer>\r\n            <div>\r\n                <button className=\"buttonPaginado\" onClick={e => handleAnterior(e)}>anterior</button>\r\n                <Paginado \r\n                    dogPerPage={dogPerPage}\r\n                    allDogs={AllDogs.length}\r\n                    paginado={paginado}\r\n                    currentPage={currentPage}\r\n                />\r\n                <button className=\"buttonPaginado\" onClick={e => handleSiguiente(e)}>siguiente</button>\r\n            </div>\r\n        </footer>\r\n        </>\r\n    )\r\n}\r\n\r\nexport default Home;","C:\\Users\\Usuario\\Documents\\Henry\\Pi\\PI-Dogs-main\\client\\src\\Components\\Detail.jsx",[],"C:\\Users\\Usuario\\Documents\\Henry\\Pi\\PI-Dogs-main\\client\\src\\Components\\Creation.jsx",[],"C:\\Users\\Usuario\\Documents\\Henry\\Pi\\PI-Dogs-main\\client\\src\\Components\\SearchBar.jsx",[],"C:\\Users\\Usuario\\Documents\\Henry\\Pi\\PI-Dogs-main\\client\\src\\Redux\\Store\\Store.jsx",[],"C:\\Users\\Usuario\\Documents\\Henry\\Pi\\PI-Dogs-main\\client\\src\\Redux\\Reducer\\Reducer.jsx",[],"C:\\Users\\Usuario\\Documents\\Henry\\Pi\\PI-Dogs-main\\client\\src\\Redux\\Actions\\Action.jsx",[],"C:\\Users\\Usuario\\Documents\\Henry\\Pi\\PI-Dogs-main\\client\\src\\Components\\Card.jsx",["73"],"import React from \"react\";\r\nimport './Card.css';\r\nfunction Card({name, temperament, img}) {\r\n    return (\r\n        <div className=\"card\">\r\n